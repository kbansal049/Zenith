<?xml version="1.0"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
	<apiVersion>57</apiVersion>
	<isExposed>true</isExposed>
	<masterLabel>zsFooterComponentLWC</masterLabel>
	<description>This is a LWC component for Zenith Community Footer</description>
	<targets>
		<target>lightning__HomePage</target>
		<target>lightning__AppPage</target>
		<target>lightning__Tab</target>
		<target>lightningCommunity__Page</target>
		<target>lightningCommunity__Default</target>
	</targets>
	<targetConfigs>
		<targetConfig targets="lightningCommunity__Default">
			<property name="facebook" type="String" label="Facebook Link" description="Enter Page link"/>
			<property name="linkedIn" type="String" label="LinkedIn Link" description="Enter Page link"/>
			<property name="twitter" type="String" label="Twitter Link" description="Enter Page link"/>
			<property name="youTube" type="String"  label="YouTube Link" description="Enter Page link"/>
			<property name="privacyUrl" type="String" label="Privacy Link" description="Enter Page link"/>
			<property name="termsUrl" type="String"  label="Terms of service Link" description="Enter Page link"/>
			<property name="aboutUrl" type="String"  label="About us Link" description="Enter Page link"/>
			<property name="faqUrl" type="String" label="FAQ Link" description="Enter Page link"/>
		</targetConfig>
	</targetConfigs>
</LightningComponentBundle>